<!--  EmpMapper.xml   -->
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ttc.po.Emp">
    <select id="getEmp" resultType="com.ttc.po.Emp">
        SELECT * FROM `Emp` e  where  e.empid = 1
    </select>

</mapper>